    "version": "0.2.0",
    "configurations": [

        {
            "name": "debug wincuts",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "GOOS": "windows",
            },
            "args": [
                "-debug"
            ]
        },
        {
            "name": "list 6",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "-list",
                "6"
            ],

            "env": {
                "GOOS": "windows",
                "GOARCH": "amd64"
            }


        },
        {
            "name": "hide 6",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [


                "-hidedesktop",
                "6"
            ],


            "env": {
                "GOOS": "windows",
                "GOARCH": "amd64"
            }
        },
        {
            "name": "show 6",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "-showdesktop",
                "6"
            ],

            "env": {
                "GOOS": "windows",
                "GOARCH": "amd64"
            }
        }
    ]


}